        "options": {}
      },
      "id": "b8c9d0e1-0008-4008-8008-000000000008",
      "name": "HubSpot — Cold Lead",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [1440, 520]
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ ({\n  success: true,\n  score: $('Parse Score').first().json.score,\n  message: 'Lead classified as ' + $('Parse Score').first().json.score\n}) }}",
        "options": {
          "responseCode": 200
        }
      },
      "id": "c9d0e1f2-0009-4009-8009-000000000009",
