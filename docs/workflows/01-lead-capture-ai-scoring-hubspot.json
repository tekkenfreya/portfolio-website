        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={{ ({\n  properties: {\n    firstname: $json.firstName,\n    lastname: $json.lastName,\n    email: $json.email,\n    phone: $json.phone,\n    company: $json.company,\n    hs_lead_status: 'OPEN',\n    description: 'AI Score: warm — ' + $json.reason,\n    message: $json.message\n  }\n}) }}",
        "options": {}
      },
